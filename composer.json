{
    "name": "nucleos/profile-bundle",
    "description": "Registration and profile management for symfony",
    "license": "MIT",
    "type": "symfony-bundle",
    "keywords": [
        "user management",
        "profile",
        "registration",
        "symfony"
    ],
    "authors": [
        {
            "name": "Christian Gripp",
            "email": "mail@core23.de"
        }
    ],
    "homepage": "https://nucleos.rocks",
    "funding": [
        {
            "type": "github",
            "url": "https://github.com/sponsors/core23"
        },
        {
            "type": "opencollective",
            "url": "https://opencollective.com/core23"
        },
        {
            "type": "ko-fi",
            "url": "https://ko-fi.com/core23"
        },
        {
            "type": "other",
            "url": "https://donate.core23.de"
        }
    ],
    "require": {
        "php": "^8.0",
        "nucleos/user-bundle": "^1.7",
        "symfony/config": "^4.4 || ^5.0",
        "symfony/dependency-injection": "^4.4 || ^5.0",
        "symfony/event-dispatcher": "^4.4 || ^5.0",
        "symfony/event-dispatcher-contracts": "^1.0 || ^2.0",
        "symfony/form": "^4.4 || ^5.0",
        "symfony/framework-bundle": "^4.4 || ^5.0",
        "symfony/http-foundation": "^4.4 || ^5.0",
        "symfony/http-kernel": "^4.4 || ^5.0",
        "symfony/intl": "^4.4 || ^5.0",
        "symfony/mailer": "^4.4 || ^5.0",
        "symfony/mime": "^4.4 || ^5.0",
        "symfony/options-resolver": "^4.4 || ^5.0",
        "symfony/routing": "^4.4 || ^5.0",
        "symfony/security-core": "^4.4 || ^5.0",
        "symfony/translation": "^4.4 || ^5.0",
        "symfony/translation-contracts": "^1.0 || ^2.0",
        "symfony/twig-bridge": "^4.4 || ^5.0",
        "symfony/twig-bundle": "^4.4 || ^5.0",
        "symfony/validator": "^4.4 || ^5.0",
        "twig/twig": "^2.14 || ^3.1"
    },
    "require-dev": {
        "alcaeus/mongo-php-adapter": "^1.1",
        "doctrine/doctrine-bundle": "^1.12 || ^2.0",
        "doctrine/orm": "^2.7",
        "ergebnis/composer-normalize": "^2.0.1",
        "symfony/browser-kit": "^4.4 || ^5.0",
        "symfony/doctrine-bridge": "^4.4 || ^5.0",
        "symfony/yaml": "^4.4 || ^5.0"
    },
    "conflict": {
        "doctrine/doctrine-bundle": "<1.12",
        "doctrine/orm": "<2.7",
        "symfony/doctrine-bridge": "<4.4"
    },
    "autoload": {
        "psr-4": {
            "Nucleos\\ProfileBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Nucleos\\ProfileBundle\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    }
}
